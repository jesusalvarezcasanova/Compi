<?xml version="1.0" encoding="UTF-8"?>
<pipeline xmlns="http://www.esei.uvigo.es/pipeline" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<programs>
		<program id="ID-1">
			<foreach element="file" source="/tmp" as="oneTempFile" />
			<exec>
				mvn -p /bin ${example1} ${i} program1
			</exec>
		</program>
		<program id="ID-2" dependsOn="ID-1" fileLog="output.txt">
			<foreach element="file" source="/tmp" as="oneTempFile2" />
			<exec>
				ls -lr ${example2} ${i} program2
			</exec>
		</program>
		<program id="ID-3" dependsOn="ID-2" fileErrorLog="error.txt">
			<exec>
				/usr/bin ${example3} program3
			</exec>
		</program>
		<program id="ID-4" dependsOn="ID-2">
			<exec>
				/usr/bin ${example4} program4
			</exec>
		</program>
		<!-- El programa 5 contiene espacios en ID y dependsON -->
		<program id="ID-5   " dependsOn="    ID-3   , ID-4    ">
			<exec>
				/usr/bin program5
			</exec>
		</program>
		<program id="ID-7" dependsOn="ID-6,ID-5">
			<exec>
				/usr/bin program7
			</exec>
		</program>
		<program id="ID-6">
			<exec>
				/usr/bin program6
			</exec>
		</program>
		<program id="ID-8" dependsOn="ID-7">
			<exec>
				/usr/bin ${example8} program8
			</exec>
		</program>
		<program id="programa9" dependsOn="ID-7">
			<exec>
				/usr/bin program9
			</exec>
		</program>
		<program id="NODE10" dependsOn="ID-8">
			<exec>
				/usr/bin program10
			</exec>
		</program>
	</programs>
	<params>
		<param type="String" name="exampleParam1" />
		<param type="Number" name="exampleParam2" />
	</params>
</pipeline>