<?xml version="1.0" encoding="UTF-8"?>
<pipeline xmlns="http://www.esei.uvigo.es/pipeline"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<programs>
		<program id="ID-1">
			<foreach element="file" source="/tmp" as="oneTempFile"/>
			<exec>
				comand -p /bin ${example}
			</exec>
		</program>
		<program id="ID-2" dependsOn="ID-1">
			<foreach element="file" source="/tmp" as="oneTempFile2"/>
			<exec>
				ls -lr ${example2}
			</exec>
		</program>
		<program id="ID-3" dependsOn="ID-1,ID-4">
			<foreach element="file" source="/tmp" as="oneTempFile3"/>
			<exec>
				/usr/bin ${example3}
			</exec>
		</program>
	</programs>
	<params>
		<param type="String" name="exampleParam1"/>
		<param type="Number" name="exampleParam2"/>
	</params>
</pipeline>